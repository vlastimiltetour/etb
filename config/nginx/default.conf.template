# upstream for uWSGI
upstream uwsgi_app {
    server unix:/code/efir/uwsgi_app.sock;
    }
server {
    listen       80;
    error_log    stderr warn;
    access_log   /dev/stdout main;

    
    location / {
        include      /etc/nginx/uwsgi_params;
        uwsgi_pass   uwsgi_app;
    }
    location /static/ {
        alias /code/efir/static/;
    }
    location /media/ {
        alias /code/efir/media/;
    }
}